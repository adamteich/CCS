

cash= 10000;
reward=0;
index=1;
results=[];

while cash>0 && index<101
hand_1= rand();
hand_2=rand();


%maybe do a RL learning thing here to decide the action? Need to go to
%notes before I implement it


action= hand_1 +rand(); %quality of hand +noise parameter, needs to be smaller but ok for now


if action >1 %fold
    cash=cash-100;
    results(index)=0;
end


if action <1 %bet
    if hand_1>hand_2 %if we lose
        cash=cash-5000;
        results(index)=-1;
    else %if we win
        cash=cash+5000;
        results(index)=1;
    end

    
    
end

cash;
index=index+1;
nrtrials=index
end


